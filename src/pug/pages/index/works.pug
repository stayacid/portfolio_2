    //Orientir
    .works-card.wow.fadeIn(style='visibility: hidden; animation-name: none;')
      img.b-lazy(src='data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==', data-src='img/portfolio/portfolio_01.jpg', alt='Сайт ОРИЕНТИР')
