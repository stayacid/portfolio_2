          a.works-card__links_button.works-card__links_view(data-fancybox='gallery', href='img/preview/preview_1.jpg')
            ion-icon(name='search')
          a.works-card__links_button.works-card__links_go(href='https://orientir-spb.ru', rel='nofollow', target='_blank')
            ion-icon(name='redo')
